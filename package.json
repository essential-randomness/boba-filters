{
  "name": "boba-filters",
  "version": "0.0.1",
  "description": "Unconventional image filters. Powered by BobaBoard and April 1st energy.",
  "main": "dist/index.js",
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:types && npm run build:js",
