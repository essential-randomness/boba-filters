    "build:js": "webpack --mode=production",
    "dev": "webpack-dev-server --open --progress --mode=development"
  },
  "author": "Essential Randomness",
  "license": "MIT",
  "homepage": "https://github.com/essential-randomness/boba-filters",
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.19.3",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-regenerator": "^7.18.6",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-typescript": "^7.18.6",
    "babel-loader": "^8.2.5",
    "css-loader": "^4.3.0",
    "file-loader": "^6.2.0",
    "style-loader": "^2.0.0",
    "typescript": "^4.8.4",
    "url-loader": "^4.1.1",
    "webpack": "^4.46.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.2.0",
    "@types/webpack-env": "^1.18.0",
